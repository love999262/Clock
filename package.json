{
  "name": "kaguya-clock",
  "version": "1.0.4",
  "description": "A easy, light, configurable, multiplatform, JavaScript clock plugin, support TypeScript/react/vue/ng/jquery/Vanilla JS/ect.",
  "main": "dist/clock.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --colors",
    "watch": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --watch  --progress --display-error-details --colors",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js --watch --colors",
    "test": "tslint -p ./ ./src/**/*.ts ./src/**/*.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/love999262/Clock.git"
  },
  "keywords": [
    "javascript",
    "clock"
  ],
  "author": "Houraisan Kaguya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/love999262/Clock/issues"
  },
  "homepage": "https://github.com/love999262/Clock#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "sass": "^1.10.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "supports-color": "^5.4.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
